#include<stdio.h>    
int main()
{    
char c;    
printf("Please enter any single character:");  
scanf("%s",&c); 
switch(c)
{
case 'a':
printf("a is  vowel");
break;
case 'e':
printf("e is  vowel");
break;
case 'i':
printf("i is  vowel");
break;
case 'o':
printf("o is  vowel");
break;
case 'u':
printf("u is  vowel");
break;
default:
printf("its consonant");
}
return 0;  
}    
